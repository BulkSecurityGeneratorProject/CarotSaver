<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Menu.
    -->
    <changeSet id="20180108090453-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="waste_metric_id"
                                 baseTableName="menu"
                                 constraintName="fk_menu_waste_metric_id"
                                 referencedColumnNames="id"
                                 referencedTableName="waste_metric"/>

        <addForeignKeyConstraint baseColumnNames="menus_id"
                                 baseTableName="menu_meals"
                                 constraintName="fk_menu_meals_menus_id"
                                 referencedColumnNames="id"
                                 referencedTableName="menu"/>
        <addForeignKeyConstraint baseColumnNames="meals_id"
                                 baseTableName="menu_meals"
                                 constraintName="fk_menu_meals_meals_id"
                                 referencedColumnNames="id"
                                 referencedTableName="meal"/>
        
    </changeSet>
</databaseChangeLog>
